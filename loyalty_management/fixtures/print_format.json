  "html": "{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\n    {% if letter_head and not no_letterhead %}\n    <div class=\"letter-head\">{{ letter_head }}</div>\n    {% endif %}\n    {%- if doc.meta.is_submittable and doc.docstatus==0-%}\n    <div class=\"alert alert-info text-center\">\n        <h4 style=\"margin: 0px;\">{{ _(\"DRAFT\") }}</h4></div>\n    {%- endif -%}\n    {%- if doc.meta.is_submittable and doc.docstatus==2-%}\n    <div class=\"alert alert-danger text-center\">\n        <h4 style=\"margin: 0px;\">{{ _(\"CANCELLED\") }}</h4></div>\n    {%- endif -%}\n    {% if max_pages > 1 %}\n    <p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\n    {% endif %}\n{%- endmacro -%}\n\n{{ add_header(0,1,doc,letter_head, no_letterhead) }}\n<style>\n    table {\n        border-collapse: collapse;\n        border: 1px solid black;\n        /*border-width: thick;*/\n    }\n    tr, td {\n        border: 1px solid black;\n        font-size: 9px;\n    }\n    div.pfcheckbox {\n        border: 1px solid black;\n        width: 15px;\n        height: 15px;\n        margin: 2px;\n        box-shadow: 1px 1px;\n    }\n</style>\n\n<!-- table with 10 columns -->\n<table>\n    <tr>\n        <td rowspan=\"5\" colspan=\"2\"></td>\n        <td rowspan=\"4\" colspan=\"5\" align=\"center\"><b>TAX INVOICE CUM CHALLAN</b></td>\n    </tr>\n    <tr>\n        <td><div class=\"pfcheckbox\"></div></td>\n        <td colspan=\"2\">ORIGINAL FOR BUYER</td>\n    </tr>\n    <tr>\n        <td><div class=\"pfcheckbox\"></div></td>\n        <td colspan=\"2\">Duplicate for Transporter</td>\n    </tr>\n    <tr>\n        <td><div class=\"pfcheckbox\"></div></td>\n        <td colspan=\"2\">Triplicate for Assessee</td>\n    </tr>\n    <tr>\n        <td></td>\n        <td colspan=\"3\" align=\"center\"><b>VALID FOR INPUTS TAX<b></td>\n        <td></td>\n        <td><div class=\"pfcheckbox\"></div></td>\n        <td colspan=\"2\">Triplicate for Assessee</td>\n    </tr>\n    <tr>\n        <td colspan=\"10\">\n            <center><b>For removal of excisable goods from a factory or warehouse with payment of duty for home consumption or export <br>\n(Vide Rule 11 of the Central Excise Rules, 2002 )</b></center>\n\n        </td>\n    </tr>\n    <tr>\n        <td colspan=\"10\" align=\"center\">\n            <b>MODERN ENGINEERING WORKS\n            <br>Savantpur Vasaht, Kirloskarwadi -416308\n            <br>District: Sangli, Taluka: Palus\n            <br>Maharashtra, India.\n            <br>Ph: +91 2346 222038,  E-mail : info@modernvalves.com</b>\n        </td>\n    </tr>\n\n    <!-- Other Details -->\n    <tr>\n        <td colspan=\"2\">Range:</td>\n        <td colspan=\"3\"></td>\n        <td colspan=\"2\"><b>ECC & CRE No</b></td>\n        <td colspan=\"3\"></td>\n    </tr>\n    <tr>\n        <td colspan=\"2\">Division</td>\n        <td colspan=\"3\"></td>\n        <td colspan=\"2\">Name of Excisable Commodity:</td>\n        <td colspan=\"3\"></td>\n    </tr>\n    <tr>\n        <td colspan=\"2\"></td>\n        <td colspan=\"3\"></td>\n        <td colspan=\"2\">Tariff Heading No:</td>\n        <td colspan=\"3\"></td>\n    </tr>\n    <tr>\n        <td colspan=\"2\">Commissionerate:</td>\n        <td colspan=\"3\"></td>\n        <td colspan=\"2\">PLA No:</td>\n        <td colspan=\"3\"></td>\n    </tr>\n    <tr>\n        <td colspan=\"2\">Pan No:</td>\n        <td colspan=\"3\"></td>\n        <td colspan=\"1\">TIN:</td>\n        <td colspan=\"1.5\">TIN NUMBER</td>\n        <td colspan=\"1\">CST:</td>\n        <td colspan=\"2.5\"></td>\n    </tr>\n    <!-- Date Details -->\n    <!-- <tr>\n        <td colspan=\"2\">Date Of Removal</td>\n        <td colspan=\"3\"></td>\n        <td colspan=\"1\">Invoice Number:</td>\n        <td colspan=\"1.5\"></td>\n        <td colspan=\"1\">P.O. No:</td>\n        <td colspan=\"2.5\"></td>\n    </tr>\n    <tr>\n        <td colspan=\"2\">Time Of Removal</td>\n        <td colspan=\"3\"></td>\n        <td colspan=\"1\">Invoice Date:</td>\n        <td colspan=\"1.5\"></td>\n        <td colspan=\"1\"></td>\n        <td colspan=\"2.5\"></td>\n    </tr>\n    <tr>\n        <td colspan=\"2\">In Words</td>\n        <td colspan=\"3\"></td>\n        <td colspan=\"1\">Invoice Time:</td>\n        <td colspan=\"1.5\"></td>\n        <td colspan=\"1\">Date</td>\n        <td colspan=\"2.5\"></td>\n    </tr> -->\n\n    <tr>\n        <td colspan=\"5\">\n            Date Of Removal: <!--date of removal-->\n            <br>Time Of Removal: <!--time of removal-->\n            <br>In Words <!--in words-->\n        </td>\n        <td colspan=\"3\">\n            Invoice Number: <!--invoice number-->\n            <br>Invoice Date: <!--invoice date-->\n            <br>Invoice Time: <!--invoice time-->\n        </td>\n        <td colspan=\"2\">\n            P.O. No:\n            <br>\n            <br>Date: <!--date-->\n        </td>\n    </tr>\n\n    <!-- Address information -->\n    <tr>\n        <td colspan=\"5\">\n            <b>Name & Address of the Buyer:</b><br>\n            <!-- Buyer's Address -->\n        </td>\n        <td colspan=\"5\">\n            <b>Name & Address of the Consignee:</b><br>\n            <!-- consignee's Address -->\n        </td>\n    </tr>\n\n    <!-- Buyer Info -->\n\n    <tr>\n        <td colspan=\"5\">Buyer's ECC No: </td>\n        <td colspan=\"1\">TIN:</td>\n        <td colspan=\"1.5\"></td>\n        <td colspan=\"1\">CST</td>\n        <td colspan=\"2.5\"></td>\n    </tr>\n    <!-- ITEM -->\n    <tr>\n        <td></td>\n        <td></td>\n        <td></td>\n        <td></td>\n        <td></td>\n        <td></td>\n        <td></td>\n        <td></td>\n        <td></td>\n        <td></td>\n    </tr>\n\n    <!-- Total and tax details -->\n    <tr>\n        <td colspan=\"3\" rowspan=\"2\"></td>\n        <td rowspan=\"2\">Total</td>\n        <td></td>\n        <td></td>\n        <td colspan=\"2\" rowspan=\"2\">\n            SUBTOTAL\n            <br>P&F\n        </td>\n        <td rowspan=\"2\"></td>\n        <td rowspan=\"2\" align=\"right\">\n            <!--subtotal-->\n            <br>\n            <!--p&f-->\n        </td>\n    </tr>\n    <tr>\n        <td>Kgs</td>\n        <td>nos</td>\n        <!-- <td colspan=\"2\">P & F</td>\n        <td></td>\n        <td></td> -->\n    </tr>\n\n    <tr>\n        <td rowspan=\"3\" colspan=\"6\"></td>\n        <td colspan=\"3\">ASS. Value</td>\n        <td></td>\n    </tr>\n    <tr>\n        <td>Excise @</td>\n        <td><!-- Percentage --></td>\n        <td></td>\n        <td><!-- Percentage --></td>\n    </tr>\n    <tr>\n        <td><b>TOTAL ED:</b></td>\n        <td></td>\n        <td></td>\n        <td><!-- value --></td>\n    </tr>\n\n    <tr>\n        <td rowspan=\"3\" colspan=\"6\"></td>\n        <td colspan=\"3\">TAXABLE VALUE</td>\n        <td></td>\n    </tr>\n    <tr>\n        <td colspan=\"2\">VAT / CST</td>\n        <td></td>\n        <td><!-- value --></td>\n    </tr>\n    <tr>\n        <td colspan=\"2\"><b>TOTAL INVOICE VALUE</b></td>\n        <td><!-- currency --></td>\n        <td><!-- value --></td>\n    </tr>\n\n    <tr>\n        <td colspan=\"2\">Total ED Payable in Words<br>Total Invoice value in Words</td>\n        <td colspan=\"8\"><!-- ed value --><br><!-- inv value --></td>\n    </tr>\n\n    <tr>\n        <td colspan=\"5\">\n            Sl.No. & date of debit entry for duty in <br>\n            - PLA <br>\n            - CENVAT CREDIT Payable\n            </noscript>\n        </td>\n        <td colspan=\"5\">Certified that the particulars given above are true and correct and the amount indicated represents <br>\n            the price actually charged and that there is no additional consideration.</td>\n    </tr>\n\n    <tr>\n        <td>Place</td>\n        <td><!-- value --></td>\n        <td colspan=\"3\" rowspan=\"2\">L.R. No:</td>\n        <td rowspan=\"4\" colspan=\"5\">\n            I/We herby certify that My/Our Registration Certificate under the <b>Maharashtra Value Added Tax Act<br>\n            2002</b> is in force on the date on which the sales of the goods specified in the tax invoice is made by<br>\n            me/us and that the transaction of sale covered by this bill/cash memorandum has been effected by <br>\n            me and it shall be accounted for in the turnover of sale while filing my return.\n        </td>\n    </tr>\n    <tr>\n        <td>Mode of Transport</td>\n        <td><!-- value --></td>\n    </tr>\n    <tr>\n        <td>Vehical No</td>\n        <td><!-- value --></td>\n        <td colspan=\"3\" rowspan=\"2\">LR dt.</td>\n    </tr>\n    <tr>\n        <td>Name of Transporter</td>\n        <td><!-- value --></td>\n    </tr>\n\n    <!-- Bank Detail -->\n    <tr>\n        <td colspan=\"2\"><b>OUR BANK DETAILS</b></td>\n        <td>Name of Benficiary:</td>\n        <td colspan=\"3\"><!-- Name of Benficiary: --></td>\n        <td rowspan=\"5\" colspan=\"4\" align=\"center\">\n            <b>For MODERN ENGINEERING WORKS\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>Signature Of Proprietor</b>\n        </td>\n    </tr>\n    <tr>\n        <td colspan=\"2\"><b>BANK AND ADDRESS:</b></td>\n        <td colspan=\"4\"><b><!--bank--></b></td>\n    </tr>\n    <tr>\n        <td colspan=\"2\"><b>Account Type and No</b></td>\n        <td><b>Current A/C No:</b></td>\n        <td colspan=\"3\"><b><!--Account No--></b></td>\n    </tr>\n    <tr>\n        <td colspan=\"2\"><b>IFSC Code</b></td>\n        <td></td>\n        <td colspan=\"3\"><b><!--ifsc code--></b></td>\n    </tr>\n    <tr>\n        <td colspan=\"6\"><!--terms and conditions-->Terms and Conditions</td>\n    </tr>\n</table>\n", 
